@page
@model SemesterProjekt.Pages.Clubmember.EditMemberModel
@{
    ViewData["Title"] = "Update member";
}
<h1>Edit clubmember</h1>

<div class="row">
    <div class="col-md-4">
        <form method="post" enctype="multipart/form-data">
            <input asp-for=@Model.ClubMember.Id type="hidden" />
            <div class="form-group">
                <label asp-for="@Model.ClubMember.Id" class="control-label"></label>
                <input asp-for="@Model.ClubMember.Id" class="control-label" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.ClubMember.Name" class="control-label"></label>
                <input asp-for="@Model.ClubMember.Name" class="control-label" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.ClubMember.Age" class="control-label"></label>
                <input asp-for="@Model.ClubMember.Age" class="control-label" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.ClubMember.Email" class="control-label"></label>
                <input asp-for="@Model.ClubMember.Email" class="control-label" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.ClubMember.Password" class="control-label"></label>
                <input asp-for="@Model.ClubMember.Password" class="control-label" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.Photo" class="control-label">Image</label>
                <div class="custom-file">
                    <input asp-for="@Model.Photo" class="custom-file-input form-control" type="file" />
                </div>
            </div>
            <div>
                <input type="submit" value="Edit" class="btn btn-primary" />
                <a asp-page="Index" class="btn cancelbutton">Cancel</a>
            </div>
        </form>
    </div>
</div>
