@page
@model SemesterProjekt.Pages.RentalPeriods.RentalIndexModel
@{
    ViewData["title"] = "RentalIndex";
}
<h1>Rental list</h1>

<table class="table">
    <thead>
        <tr>
            <td> Id </td>
            <td> from the </td>
            <td> to the </td>
        </tr>
    </thead>
    <tbody>
        <tr>
            @foreach(var item in Model.Periods)
            {
                <td>@item.Id</td>
                <td>@item.RentalperiodFrom</td>
                <td>@item.RentalperiodTo</td>
            }
        </tr>
    </tbody>
</table>